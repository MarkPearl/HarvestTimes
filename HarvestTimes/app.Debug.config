<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
   <add key="HarvestUserName" value="Example User Name" xdt:Locator="Match(key)" xdt:Transform="Replace"/>
   <add key="HarvestPassword" value="Example Password" xdt:Locator="Match(key)" xdt:Transform="Replace"/>
   <add key="HarvestUserId" value="Example User Id" xdt:Locator="Match(key)" xdt:Transform="Replace"/>
   <add key="HarvestCompanyUrl" value="https://ExampleCompanyUrl.harvestapp.com/" xdt:Locator="Match(key)" xdt:Transform="Replace"/>
  </appSettings>
</configuration>
